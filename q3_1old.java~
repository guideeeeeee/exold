import java.util.Scanner;
public class q3_1old{
  public static void main (String[]args){
    Scanner sn = new Scanner(System.in);
    int num ;
    double t =0;
    for(int i=0;i<50;i++){
    System.out.print("Enter number:");
    num = sn.nextInt();
    t +=(double)num/(num+2);
    }
    System.out.println("T = "+t);
  }
}