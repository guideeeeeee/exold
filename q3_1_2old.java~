import java.util.Scanner;
public class q3_1_2old{
  public static void main (String[]args){
    Scanner sn = new Scanner(System.in);
    int num ;
    double t;
    System.out.print("Enter number:");
    num = sn.nextInt();
    for(int i=0;i<=num;i++){
      if(i%2==0)
        t=-1/(2*i-1);
    if(i%50==0)
      System.out.printf("T = %.3f\n",t);
    }
  }
}