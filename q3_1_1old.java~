import java.util.Scanner;
public class q3_1_1old{
  public static void main (String[]args){
    Scanner sn = new Scanner(System.in);
    int num ;
    double t =0;
    System.out.print("Enter number:");
    num = sn.nextInt();
    for(int i=0;i<num;i++){
      t=i/(i+2);
    if(num%50==0)
      System.out.println("T = "+t);
    }
  }
}